CC ?= gcc
OBJCOPY ?= objcopy
CROSS_COMPILE ?= arm-none-gnueabi-
override CFLAGS := $(CFLAGS) -g -Os -fpic -fno-common -fno-builtin -ffreestanding -nostdinc -mno-thumb-interwork -Wall -Wstrict-prototypes -fno-stack-protector -Wno-format-nonliteral -Wno-format-security -fno-toplevel-reorder
override CC := $(CROSS_COMPILE)$(CC)
override OBJCOPY := $(CROSS_COMPILE)$(OBJCOPY)
